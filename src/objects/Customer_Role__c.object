    <validationRules>
        <fullName>Cannot_Create_CustomerRole</fullName>
        <active>true</active>
        <description>Cannot create a customer role if a contact does not have a primary Address, a primary email, or a primary phone.</description>
        <errorConditionFormula>NOT(BEGINS($Profile.Name, &quot;API -&quot;)) &amp;&amp; NOT( ISBLANK( Contact__r.Id) ) &amp;&amp; (ISBLANK(Contact__r.Phone) &amp;&amp;  ISBLANK( Contact__r.Email) &amp;&amp;  ISBLANK( Contact__r.MailingCity) &amp;&amp; ISBLANK(Contact__r.MailingCountry))</errorConditionFormula>
        <errorMessage>Cannot create a customer role if a contact does not have a primary Address, a primary email, or a primary phone.</errorMessage>
    </validationRules>
