    <validationRules>
        <fullName>Cannot_Create_or_Update_ContactMethod</fullName>
        <active>true</active>
        <description>Cannot create a Contact Method if a contact does not have a primary email, or a primary phone.</description>
        <errorConditionFormula>NOT(BEGINS($Profile.Name, &quot;API -&quot;)) &amp;&amp; Not(ISPICKVAL( ContactMethodUsageValue__c ,&apos;Primary Phone&apos;)) &amp;&amp; Not(ISPICKVAL( ContactMethodUsageValue__c ,&apos;Primary Email&apos;)) &amp;&amp; NOT(ISBLANK( Contact__r.Id)) &amp;&amp; (ISBLANK(Contact__r.Phone) &amp;&amp; ISBLANK(Contact__r.Email) &amp;&amp;  ISBLANK(Contact__r.MailingCity) &amp;&amp; ISBLANK(Contact__r.MailingCountry))</errorConditionFormula>
        <errorMessage>Cannot create a Contact Method if a contact does not have a primary Address, a primary email, or a primary phone.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Email_Address_Format</fullName>
        <active>true</active>
        <description>If Method = &quot;Email Address&quot;, the field Value should have an email address format</description>
        <errorConditionFormula>IF(NOT(BEGINS($Profile.Name, &quot;API -&quot;)) &amp;&amp; NOT(ISBLANK(Text(ContactMethodValue__c))) &amp;&amp; ISPICKVAL(ContactMethodValue__c,&apos;Email Address&apos;), NOT(REGEX(ReferenceNumber__c ,&quot;[a-zA-Z0-9\\.\\!\\#\\$\\%\\&amp;\\*\\/\\=\\?\\^\\_\\+\\-\\`\\{\\|\\}\\~\&apos;._%+-]+@[a-zA-Z0-9\\-.-]+\\.[a-zA-Z]+&quot;)),false)</errorConditionFormula>
        <errorDisplayField>ReferenceNumber__c</errorDisplayField>
        <errorMessage>Not a valid Email Address format</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_is_greater_than_End_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(BEGINS($Profile.Name, &quot;API -&quot;)), StartDate__c &gt; EndDate__c )</errorConditionFormula>
        <errorMessage>Start Date cannot be greater than End Date</errorMessage>
    </validationRules>
